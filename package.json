{
  "name": "@namics/env-linter",
  "description": "Bunch of CLI helper for our projects. Check node version, hooks installed and more.",
  "version": "0.2.1",
  "author": "Namics AG",
  "bin": {
    "env-linter": "./bin/index.js"
  },
  "bugs": "https://github.com/namics/env-linter/issues/new",
  "commitlint": {
    "extends": [
      "@namics/commitlint-conventional-changelog"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@namics/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@dash4/plugin-actions": "0.8.5",
    "@dash4/plugin-code-coverage": "0.8.5",
    "@dash4/plugin-dependencies": "0.8.5",
    "@dash4/plugin-npm-scripts": "0.8.5",
    "@dash4/plugin-readme": "0.8.5",
    "@dash4/plugin-terminal": "0.8.5",
    "@dash4/server": "0.8.5",
    "@namics/commitlint-conventional-changelog": "1.1.16",
    "@namics/cz-conventional-changelog": "2.0.2",
    "@namics/eslint-config": "9.0.1",
    "@namics/prettier-config": "1.1.0",
    "@namics/ts-config": "1.0.1",
    "@types/chalk": "2.2.0",
    "@types/execa": "2.0.0",
    "@types/fs-extra": "9.0.1",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/node-fetch": "2.5.7",
    "@types/semver": "7.2.0",
    "@types/zen-observable": "0.8.0",
    "chalk": "4.1.0",
    "ci-info": "2.0.0",
    "clean-webpack-plugin": "3.0.0",
    "commander": "5.1.0",
    "commitizen": "4.1.2",
    "eslint": "7.2.0",
    "eslint-plugin-import": "2.21.2",
    "execa": "4.0.2",
    "find-packages": "7.0.9",
    "fs-extra": "9.0.1",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "license-checker": "25.0.1",
    "lint-staged": "10.2.9",
    "log-symbols": "4.0.0",
    "node-fetch": "2.6.0",
    "npm-run-all": "4.1.5",
    "ora": "4.0.4",
    "pkg-ok": "2.3.1",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "semver": "7.3.2",
    "standard-version": "8.0.0",
    "ts-config-webpack-plugin": "2.0.0",
    "ts-jest": "26.1.0",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "zen-observable": "0.8.15"
  },
  "engines": {
    "node": ">=8"
  },
  "files": [
    "README.md",
    "LICENSE",
    "bin",
    "build",
    "dist"
  ],
  "homepage": "https://github.com/namics/env-linter",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "cli",
    "env",
    "hooks installed",
    "namics",
    "node version",
    "save exact"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --ext .jsx,.js,.ts,.tsx"
    ]
  },
  "main": "./dist/index.js",
  "private": false,
  "repository": "https://github.com/namics/env-linter",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:bundle": "webpack --mode=\"production\"",
    "build:definitions": "tsc",
    "clean": "npx npm-run-all clean:*",
    "clean:dirs": "npx rimraf lerna-debug.log npm-debug.log package-lock.json",
    "clean:modules": "npx rimraf node_modules",
    "cz": "git-cz",
    "lint": "npm-run-all lint:*",
    "lint:license": "license-checker --production --summary --exclude \"Apache-2.0, BSD, ISC, LGPL, MIT, MPL\" --failOn \"AGPL; EPL; GPL\"",
    "lint:ts": "eslint . --ext .jsx,.js,.ts,.tsx",
    "postbuild": "rimraf build/*.js build/*js.map",
    "prebuild": "rimraf build dist",
    "prepublishOnly": "npm-run-all prepublishOnly:*",
    "prepublishOnly:lint": "npm run lint",
    "prepublishOnly:test": "npm test",
    "prepublishOnly:build": "npm run build",
    "prepublishOnly:pkg": "pkg-ok",
    "prerelease": "npm-run-all lint test build",
    "prettier": "prettier --write \"**/*.*(js|jsx|ts|tsx|json|md)\"",
    "release": "standard-version",
    "sort-package": "npx fixpack",
    "start": "dash4",
    "test": "jest --coverage",
    "watch": "run-p watch:*",
    "watch-test": "jest --watchAll --coverage --notify --notifyMode=failure-change",
    "watch:bundle": "webpack --watch --mode=\"development\"",
    "watch:definitions": "tsc -w"
  },
  "typings": "./build/index.d.ts"
}
